<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:Mapping_Tools.Desktop.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Mapping_Tools.Desktop.Views.SettingsView"
             x:DataType="vm:SettingsViewModel">

    <Design.DataContext>
        <vm:SettingsViewModel/>
    </Design.DataContext>

    <StackPanel>
        <TextBlock>Welcome to Settings!</TextBlock>
        <TextBox Text="{Binding Note, Mode=TwoWay}"></TextBox>
        <TextBox Text="{Binding Counter, Mode=TwoWay}"></TextBox>
        <TextBlock>test</TextBlock>
    </StackPanel>

</UserControl>